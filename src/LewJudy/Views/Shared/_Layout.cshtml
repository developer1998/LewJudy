@using LewJudy.Models;
@inject IClientPackagesRepository iCPRepository;
<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewBag.Title</title>
	<link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap.@(ViewBag.Debug)css?v=@(iCPRepository.GetByName("bootstrap").ID)" />
	<link rel="stylesheet" href="~/lib/bootstrap-touch-carousel/css/bootstrap-touch-carousel.@(ViewBag.Debug)css?v=@(iCPRepository.GetByName("bootstrap_touch_carousel").ID)" />
	<link rel="stylesheet" href="~/css/native/core.@(ViewBag.Debug)css?v=@(iCPRepository.GetByName("native").ID)" />
	@RenderSection("styles", required: false)
</head>
<body>
	<div class="navbar navbar-inverse navbar-fixed-top">
		<div class="container">
			<div class="navbar-header">
				<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
					<span class="icon-bar"></span>
				</button>
				@Html.ActionLink("Wspólnota Odnowy w Duchu Świętym Lew Judy", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
			</div>
			<div class="navbar-collapse collapse">
				<ul class="nav navbar-nav">
					<!--<li>@Html.ActionLink("Home", "Index", "Home")</li>-->
					<!--<li>@Html.ActionLink("About", "About", "Home")</li>-->
					<li>@Html.ActionLink("Kontakt", "Contact", "Home")</li>
				</ul>
				@await Html.PartialAsync("_LoginPartial")
			</div>
		</div>
	</div>
	<div class="container body-content">
		@RenderBody()
		<br /><br />
		<hr />
		<footer>
			<em style="font-size: smaller; float: right">&copy; @DateTime.Now.Year - Wspólnota Odnowy w Duchu Świętym Lew Judy</em>
		</footer>
	</div>
	<script src="~/lib/jquery/jquery.@(ViewBag.Debug)js?v=@(iCPRepository.GetByName("jquery").ID)"></script>
	<script src="~/lib/bootstrap/js/bootstrap.@(ViewBag.Debug)js?v=@(iCPRepository.GetByName("bootstrap").ID)"></script>
	<script src="~/lib/hammer.js/hammer.@(ViewBag.Debug)js?v=@(iCPRepository.GetByName("hammerjs").ID)"></script>
	<script src="~/lib/bootstrap-touch-carousel/js/bootstrap-touch-carousel.@(ViewBag.Debug)js?v=@(iCPRepository.GetByName("bootstrap_touch_carousel").ID)"></script>
	<script src="~/lib/native/core.@(ViewBag.Debug)js?v=@(iCPRepository.GetByName("native").ID)"></script>
    @RenderSection("scripts", required: false)
</body>
</html>